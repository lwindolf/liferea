<interface>
  <requires lib="gtk" version="4.0"/>
  <object class="GtkShortcutsWindow" id="shortcuts">
    <child>
      <object class="GtkShortcutsSection">
        <child>
          <object class="GtkShortcutsGroup">
            <property name="title">Feed List</property>
            <child>
              <object class="GtkShortcutsShortcut">
                <property name="accelerator">u</property>
                <property name="title">Select previous feed</property>
              </object>
            </child>
            <child>
              <object class="GtkShortcutsShortcut">
                <property name="accelerator">d</property>
                <property name="title">Select next feed</property>
              </object>
            </child>
            <child>
              <object class="GtkShortcutsShortcut">
                <property name="accelerator">&lt;Ctrl&gt;R</property>
                <property name="title">Mark all read</property>
              </object>
            </child>
            <child>
              <object class="GtkShortcutsShortcut">
                <property name="accelerator">&lt;Ctrl&gt;U</property>
                <property name="title">Update all</property>
              </object>
            </child>
          </object>
